<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.laladev.moneyjinn.businesslogic.dao.mapper.IMoneyflowDaoMapper">

    	<sql id="moneyflowColumns">
		 moneyflowid		AS id
		,mac_id_creator		AS macIdCreator
		,mac_id_accessor	AS macIdAccessor
		,bookingdate
		,invoicedate
		,amount
		,mcs_capitalsourceid	AS mcsCapitalsourceId
		,mcp_contractpartnerid	AS mcpContractpartnerId
		,comment
		,mpa_postingaccountid	AS mpaPostingAccountId
		,private		AS privat
	</sql>
    
	<insert id="createMoneyflow"
		parameterType="org.laladev.moneyjinn.businesslogic.dao.data.MoneyflowData"
		useGeneratedKeys="true" keyProperty="id" keyColumn="moneyflowid">
		INSERT
		  INTO moneyflows
		      (mac_id_creator
		      ,mac_id_accessor
		      ,bookingdate
		      ,invoicedate
		      ,amount
		      ,mcs_capitalsourceid
		      ,mcp_contractpartnerid
		      ,comment
		      ,mpa_postingaccountid
		      ,private
		      )
		VALUES
		      (#{macIdCreator}
		      ,#{macIdAccessor}
		      ,#{bookingdate}
		      ,#{invoicedate}
		      ,#{amount}
		      ,#{mcsCapitalsourceId}
		      ,#{mcpContractpartnerId}
		      ,#{comment}
		      ,#{mpaPostingAccountId}
		      ,#{privat}
		      )
	</insert>
</mapper>
 